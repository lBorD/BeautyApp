{
  "name": "beautyapp",
  "license": "0BSD",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npx expo start --clear --go",
    "android": "npx expo start --clear --android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "tunnel": "expo start --clear --go --tunnel",
    "build": "eas build --platform all",
    "build:android": "eas build --platform android",
    "build:apk": "eas build --platform android --type apk",
    "build:ios": "eas build --platform ios",
    "build:dev": "eas build --profile development --no-build-cache --platform android",
    "update:dev": "eas update --branch development --message \"Update development\"",
    "update:prod": "eas update --branch production --message \"Update production\""
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.19",
    "@react-navigation/native-stack": "^7.3.3",
    "@react-navigation/stack": "^7.1.1",
    "axios": "^1.8.1",
    "date-fns": "^4.1.0",
    "expo": "~53.0.0",
    "expo-dev-client": "~5.1.0",
    "expo-navigation-bar": "~4.2.4",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.0",
    "libphonenumber-js": "^1.12.8",
    "ngrok": "^5.0.0-beta.2",
    "react": "^19.0.0",
    "react-native": "^0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.13.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.3",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
